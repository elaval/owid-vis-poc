{
  "compilerOptions": {
    "sourceMap": true,
    "esModuleInterop": true,    
    "skipLibCheck": true,



    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "target": "esnext",
    "module": "esnext",
    "lib": ["esnext", "dom"],
    "strict": true,
    "stripInternal": true,
    "outDir": "dist",
    "allowJs": true,
    "declaration": true,
    "declarationDir": "types",
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "paths": {
      "@elaval/owid-demo": ["./src/index.js"]
    }
  },
  "include": ["src/**/*"]
}




 